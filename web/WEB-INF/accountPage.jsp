    <div id="main">
      <h1>Welcome, ${user}</h1>
      <h2 class="subject">Posts</h2>
      
        <c:forEach var="row" items= "${userPosts}">
          <div class="postsForm">
          <form name="postSelectorForm" action="accessPost" method="POST">
            <h3 name="title" class="title"><c:out value="${row.get(1)}"/></h3>
            <c:out value="${row.get(4)}"/>
            <input type="submit" value="View" class="viewButton">
            <input type="hidden" value="${row.get(0)}" name="postId"/>
            
          </form>
            <form name="deleteForm" action="deletePost" method="POST">
                <input type="submit" value="Delete Post" class="deleteButton">
                <input type="hidden" value="${row.get(0)}" name="postId"/>
              </form>
              
              
          </div>
        </c:forEach>




    </div>
